    "description": "codeat3's helper methods to use in blade-icon generation scripts",
    "license": "MIT",
    "require": {
        "illuminate/support": "^8.0",
        "renatomarinho/laravel-page-speed": "^2.0",
        "symfony/process": "^5.3",
        "symfony/filesystem": "^5.3"
    },
    "require-dev": {
